{
  "name": "n2-slackbot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.12.14",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "tsc": "tsc -p tsconfig.json",
    "cp": "cp -f ./package.json ./yarn.lock ./dist",
    "prodInstall": "yarn install --cwd ./dist  --production",
    "build": "npm-run-all clean tsc cp prodInstall",
    "deploy": "./deploy.sh"
  }
}
