{
  "name": "n2-slackbot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.36",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.151",
    "@types/moment-precise-range-plugin": "^0.2.0",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "test": "jest --verbose",
    "clean": "rimraf dist/*",
    "tsc": "tsc -p tsconfig.json",
    "cp": "cp -f ./package.json ./yarn.lock ./.env ./dist",
    "prodInstall": "yarn install --cwd ./dist  --production",
    "build": "npm-run-all clean tsc cp prodInstall",
    "deploy": "./deploy.sh",
    "lint": "eslint --ext .ts,.js src/",
    "lint:fix": "eslint --fix --ext .ts,.js src/"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "dependencies": {
    "@slack/web-api": "^5.6.0",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "moment-precise-range-plugin": "^1.3.0"
  }
}
